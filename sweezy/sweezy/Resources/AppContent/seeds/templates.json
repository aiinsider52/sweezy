  {
    "id": "880e8400-e29b-41d4-a716-446655440002",
    "title": "Health Insurance Application",
    "description": "Application letter for health insurance coverage",
    "category": "insurance",
    "templateType": "application",
    "content": "{{date}}\n\nTo: {{insurance_company}}\n{{insurance_address}}\n\nSubject: Application for Basic Health Insurance\n\nDear Sir/Madam,\n\nI would like to apply for basic health insurance coverage under Swiss law.\n\nPersonal Information:\nFull Name: {{fullName}}\nDate of Birth: {{dateOfBirth}}\nGender: {{gender}}\nNationality: {{nationality}}\nPermit Type: {{permitType}}\n\nContact Information:\nAddress: {{address}}\nPostal Code: {{postalCode}}\nCity: {{city}}\nPhone: {{phoneNumber}}\nEmail: {{email}}\n\nInsurance Preferences:\nDeductible: CHF {{deductible}}\nInsurance Model: {{insuranceModel}}\nStart Date: {{startDate}}\n\nI confirm that I understand my obligations under Swiss health insurance law and agree to pay premiums on time.\n\nPlease send me the necessary forms and information about premium costs.\n\nThank you for your consideration.\n\nSincerely,\n{{fullName}}\n\nEnclosures:\n- Copy of passport\n- Copy of residence permit\n- Proof of address",
    "placeholders": [
      {"id":"date","label":"Date","description":"Current date","type":"date","defaultValue":null,"options":null,"validation":null,"isRequired":true,"order":1},
      {"id":"insurance_company","label":"Insurance Company","description":"Name of the insurance company","type":"dropdown","defaultValue":null,"options":["CSS","Helsana","Swica","KPT","Sanitas","Concordia","Visana"],"validation":null,"isRequired":true,"order":2},
      {"id":"insurance_address","label":"Insurance Company Address","description":"Address of the insurance company office","type":"multilineText","defaultValue":null,"options":null,"validation":null,"isRequired":true,"order":3},
      {"id":"fullName","label":"Full Name","description":"Your complete legal name","type":"text","defaultValue":null,"options":null,"validation":null,"isRequired":true,"order":4},
      {"id":"dateOfBirth","label":"Date of Birth","description":"Your date of birth","type":"date","defaultValue":null,"options":null,"validation":null,"isRequired":true,"order":5},
      {"id":"gender","label":"Gender","description":"Your gender","type":"dropdown","defaultValue":null,"options":["Male","Female","Other"],"validation":null,"isRequired":true,"order":6},
      {"id":"nationality","label":"Nationality","description":"Your nationality","type":"text","defaultValue":"Ukrainian","options":null,"validation":null,"isRequired":true,"order":7},
      {"id":"permitType","label":"Permit Type","description":"Your residence permit type","type":"dropdown","defaultValue":"S - Protection Status","options":["S - Protection Status","B - Residence Permit","C - Settlement Permit","F - Provisional Admission","N - Asylum Seeker"],"validation":null,"isRequired":true,"order":8},
      {"id":"deductible","label":"Deductible Amount","description":"Annual deductible amount in CHF","type":"dropdown","defaultValue":"300","options":["300","500","1000","1500","2000","2500"],"validation":null,"isRequired":true,"order":9},
      {"id":"insuranceModel","label":"Insurance Model","description":"Type of insurance model","type":"dropdown","defaultValue":"Standard","options":["Standard","Family Doctor","HMO","Telmed"],"validation":null,"isRequired":true,"order":10},
      {"id":"startDate","label":"Coverage Start Date","description":"When you want coverage to begin","type":"date","defaultValue":null,"options":null,"validation":null,"isRequired":true,"order":11}
    ],
    "requiredFields": ["date","insurance_company","insurance_address","fullName","dateOfBirth","gender","nationality","permitType","deductible","insuranceModel","startDate"],
    "tags": ["health-insurance","application","mandatory"],
