      {"id": "nationality", "label": "Staatsangehörigkeit", "type": "text", "defaultValue": "Ukraine", "isRequired": true, "order": 8},
      {"id": "occupation", "label": "Beruf", "type": "text", "isRequired": true, "order": 9},
