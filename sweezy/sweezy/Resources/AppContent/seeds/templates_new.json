      {"id": "permitType", "label": "Тип разрешения", "type": "text", "defaultValue": "S - Protection Status", "isRequired": true, "order": 9},
      {"id": "familySize", "label": "Размер семьи", "type": "number", "defaultValue": "1", "isRequired": true, "order": 10},
      {"id": "monthlyIncome", "label": "Доход в месяц", "type": "number", "isRequired": true, "order": 11},
